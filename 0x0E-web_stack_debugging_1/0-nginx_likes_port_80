#!/usr/bin/env bash                                                                                                                                                                                                                                                                                                                                                                               
cd /etc/nginx/sites-enabled/ || exit;                                                                                                                                                                                                                                                            
rm default;                                                                                                                                                                                                                                           
ln -s /etc/nginx/sites-available/default default;                                                                                                                                                                                                                                                
service nginx restart 
